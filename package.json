{
    "name": "pack-format",
    "version": "1.2.5",
    "description": "Returns the pack_format of any Minecraft version, including snapshots",
    "scripts": {
        "prepublish": "tsc",
        "test": "tsc && node dist/test/test"
    },
    "keywords": [
        "minecraft",
        "resourcepack",
        "resource pack",
        "datapack",
        "data pack",
        "pack format"
    ],
    "main": "dist/src/index.js",
    "bin": {
        "pack-format": "bin/index.js"
    },
    "files": [
        "bin/",
        "dist/",
        "!dist/test/"
    ],
    "engines": {
        "node": ">=12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Nixinova/pack-format.git"
    },
    "author": "Nixinova (https://nixinova.com)",
    "bugs": {
        "url": "https://github.com/Nixinova/pack-format/issues"
    },
    "license": "ISC",
    "dependencies": {
        "yargs-parser": "^20.2.9"
    },
    "devDependencies": {
        "@types/node": "ts4.4",
        "@types/yargs-parser": "ts4.4",
        "typescript": "~4.4.3"
    }
}
